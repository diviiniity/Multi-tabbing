{
    "manifest_version": 3,
    "name": "Multi-tabbing",
    "version": "1.0",
    "icons": {
      "16": "icon/icon16.png",
      "32": "icon/icon32.png",
      "48": "icon/icon48.png",  
      "128": "icon/icon128.png"
    },
    "action": {
      "default_popup": "manager.html"
    },
    "permissions": [
      "tabGroups",
      "tabs"
    ]
  }